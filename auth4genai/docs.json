{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Auth0",
  "colors": {
    "primary": "#5001FF",
    "light": "#5001FF",
    "dark": "#341773"
  },
  "favicon": "/ai/docs/favicon.svg",
  "fonts": {
    "heading": {
      "family": "Aeonik",
      "source": "https://cdn.auth0.com/website/cic-homepage/fonts/Aeonik-Medium.woff2",
      "format": "woff2"
    },
    "body": {
      "family": "Aeonik",
      "source": "https://cdn.auth0.com/website/cic-homepage/fonts/Aeonik-Regular.woff2",
      "format": "woff2"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude"]
  },
  "appearance": {
    "default": "dark"
  },
  "background": {
    "decoration": "gradient"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Auth for GenAI",
            "pages": [
              "ai/docs/intro/overview",
              {
                "group": "User Authentication",
                "pages": [
                  "ai/docs/intro/user-authentication",
                  "ai/docs/intro/call-your-apis-on-users-behalf"
                ]
              },
              {
                "group": "Call Other's APIs with Token Vault",
                "pages": [
                  "ai/docs/intro/token-vault",
                  "ai/docs/intro/connections",
                  "ai/docs/intro/account-linking",
                  "ai/docs/intro/call-others-apis-on-users-behalf"
                ]
              },
              "ai/docs/intro/asynchronous-authorization",
              "ai/docs/intro/authorization-for-rag"
            ]
          },
          {
            "group": "Get Started",
            "pages": [
              "ai/docs/get-started/overview",
              "ai/docs/get-started/user-authentication",
              "ai/docs/get-started/call-your-apis-on-users-behalf",
              "ai/docs/get-started/call-others-apis-on-users-behalf",
              "ai/docs/get-started/asynchronous-authorization",
              "ai/docs/get-started/authorization-for-rag"
            ]
          },
          {
            "group": "Sample Apps",
            "pages": ["ai/docs/sample-apps"]
          },
          {
            "group": "Guides",
            "pages": [
              "ai/docs/guides/google-sign-in-and-auth",
              "ai/docs/guides/client-initiated-account-linking"
            ]
          },
          {
            "group": "Support",
            "pages": ["ai/docs/support"]
          },
          {
            "group": "Glossary",
            "pages": ["ai/docs/glossary"]
          }
        ]
      },
      {
        "tab": "How-Tos",
        "groups": [
          {
            "group": " ",
            "pages": [
              "ai/docs/how-tos/overview",
              "ai/docs/how-tos/check-google-calendar-availability",
              "ai/docs/how-tos/list-github-repositories",
              "ai/docs/how-tos/get-github-issues-python",
              "ai/docs/how-tos/list-slack-channels",
              "ai/docs/how-tos/get-salesforce-opportunities",
              "ai/docs/how-tos/analyze-strava-activities",
              "ai/docs/how-tos/create-spotify-playlist"
            ]
          }
        ]
      },
      {
        "tab": "Auth for MCP",
        "groups": [
          {
            "group": " ",
            "pages": ["ai/docs/mcp/auth-for-mcp", "ai/docs/mcp/cloudflare-mcp"]
          }
        ]
      },
      {
        "tab": "SDKs",
        "groups": [
          {
            "group": " ",
            "pages": [
              "ai/docs/sdks/overview",
              "ai/docs/sdks/javascript-sdk",
              "ai/docs/sdks/python-sdk",
              "ai/docs/sdks/langchain-sdk",
              "ai/docs/sdks/llamaindex-sdk",
              "ai/docs/sdks/vercel-ai-sdk",
              "ai/docs/sdks/cloudflare-sdk",
              "ai/docs/sdks/genkit-sdk"
            ]
          }
        ]
      }
    ]
  },
  "redirects": [
    {
      "source": "/ai/docs/get-started",
      "destination": "/ai/docs/get-started/overview"
    },
    {
      "source": "/ai/docs/user-authentication",
      "destination": "/ai/docs/get-started/user-authentication"
    },
    {
      "source": "/ai/docs/call-your-apis-on-users-behalf",
      "destination": "/ai/docs/get-started/call-your-apis-on-users-behalf"
    },
    {
      "source": "/ai/docs/call-others-apis-on-users-behalf",
      "destination": "/ai/docs/get-started/call-others-apis-on-users-behalf"
    },
    {
      "source": "/ai/docs/async-authorization",
      "destination": "/ai/docs/get-started/async-authorization"
    },
    {
      "source": "/ai/docs/authorization-for-rag",
      "destination": "/ai/docs/get-started/authorization-for-rag"
    },
    {
      "source": "/ai/docs/how-tos",
      "destination": "/ai/docs/how-tos/overview"
    },
    {
      "source": "/ai/docs/check-google-calendar-availability",
      "destination": "/ai/docs/how-tos/check-google-calendar-availability"
    },
    {
      "source": "/ai/docs/list-github-repositories",
      "destination": "/ai/docs/how-tos/list-github-repositories"
    },
    {
      "source": "/ai/docs/list-slack-channels",
      "destination": "/ai/docs/how-tos/list-slack-channels"
    },
    {
      "source": "/ai/docs/get-salesforce-opportunities",
      "destination": "/ai/docs/how-tos/get-salesforce-opportunities"
    },
    {
      "source": "/ai/docs/auth-for-mcp",
      "destination": "/ai/docs/mcp/auth-for-mcp"
    },
    {
      "source": "/ai/docs/cloudflare-mcp",
      "destination": "/ai/docs/mcp/cloudflare-mcp"
    },
    {
      "source": "/ai/docs/sdks",
      "destination": "/ai/docs/sdks/overview"
    },
    {
      "source": "/ai/docs/google-sign-in-and-auth",
      "destination": "/ai/docs/guides/google-sign-in-and-auth"
    },
    {
      "source": "/ai/docs/client-initiated-account-linking",
      "destination": "/ai/docs/guides/client-initiated-account-linking"
    }
  ],
  "logo": {
    "light": "/ai/docs/logo/light.svg",
    "dark": "/ai/docs/logo/dark.svg",
    "href": "https://auth0.com/"
  },
  "navbar": {
    "links": [
      {
        "label": "Sign up",
        "href": "https://auth0.com/signup?place=header&type=button&text=sign%20up"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Contact sales",
      "href": "https://auth0.com/contact-us?place=header&type=button&text=contact%20sales"
    }
  },
  "footer": {
    "socials": {
      "x": "https://twitter.com/auth0",
      "github": "https://github.com/auth0",
      "linkedin": "https://linkedin.com/company/auth0"
    }
  }
}
